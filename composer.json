{
    "name": "tempest/view",
    "description": "A component for view handling.",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": "^8.4",
        "tempest/core": "1.0.0-alpha.6",
        "tempest/container": "1.0.0-alpha.6",
        "tempest/validation": "1.0.0-alpha.6",
        "tempest/cache": "1.0.0-alpha.6",
        "tempest/highlight": "^2.11.2"
    },
    "autoload": {
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "Tempest\\View\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tempest\\View\\Tests\\": "tests"
        }
    }
}
